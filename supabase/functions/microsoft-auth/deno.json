{
  "imports": {
    "@supabase/supabase-js": "npm:@supabase/supabase-js@2",
    "@azure/identity": "npm:@azure/identity@4"
  },
  "compilerOptions": {
    "allowJs": true,
    "lib": ["deno.ns", "deno.window", "es2022"],
    "strict": true
  },
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "exclude": ["no-explicit-any"]
    }
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 120,
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": true,
    "proseWrap": "preserve"
  }
}
